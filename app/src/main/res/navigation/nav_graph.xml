            android:id="@+id/action_statistics2_to_tags2"
            app:destination="@id/tags" />
    </fragment>
    <fragment
        android:id="@+id/tags"
        android:name="com.oristats.ui.main.Tags"
        android:label="tags_fragment"
        tools:layout="@layout/tags_fragment" >
        <action
            android:id="@+id/action_tags2_to_statistics2"
            app:destination="@id/statistics" />
